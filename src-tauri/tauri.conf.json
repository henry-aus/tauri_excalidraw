{
  "productName": "hackernews",
  "version": "0.0.0",
  "identifier": "com.acme.hackernews",
  "build": {
    "beforeDevCommand": "cd ../excalidraw && yarn start",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "cd ../excalidraw && yarn start:app:production",
    "frontendDist": "../excalidraw/excalidraw-app/build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
